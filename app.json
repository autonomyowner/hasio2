{
  "expo": {
    "name": "Hasio",
    "slug": "hasio",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "hasio",
    "icon": "./assets/IconKitchen-Output/android/play_store_512.png",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/IconKitchen-Output/android/play_store_512.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF7F2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.hasio",
      "icon": "./assets/IconKitchen-Output/ios/AppIcon@2x.png",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Hasio needs access to your microphone to enable voice conversations with the travel assistant.",
        "NSPhotoLibraryUsageDescription": "Hasio needs access to your photos to save and share your travel moments.",
        "NSCameraUsageDescription": "Hasio needs access to your camera to capture travel moments."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundImage": "./assets/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "monochromeImage": "./assets/IconKitchen-Output/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
        "backgroundColor": "#F5A623"
      },
      "package": "app.rork.hasio",
      "versionCode": 4,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/IconKitchen-Output/web/favicon.ico"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      [
        "expo-av",
        {
          "microphonePermission": "Hasio needs access to your microphone to enable voice conversations with the travel assistant."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "autonomy",
    "extra": {
      "eas": {
        "projectId": "8859775e-cedf-45b7-aa94-3c7cbfb7be12"
      },
      "router": {}
    }
  }
}
